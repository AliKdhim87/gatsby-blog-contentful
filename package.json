{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "develop": "env-cmd -f .env.development gatsby develop",
    "start": "env-cmd -f .env.development run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/functions",
    "build": "env-cmd -f .env.development gatsby build && netlify-lambda build src/functions",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@hookform/resolvers": "^1.2.0",
    "@sendgrid/mail": "^7.4.0",
    "gatsby": "^2.24.85",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-layout": "^1.3.13",
    "gatsby-plugin-less": "^4.3.0",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-react-i18next": "0.0.27",
    "gatsby-plugin-sass": "^2.3.22",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-styled-components": "^3.6.0",
    "gatsby-plugin-typescript": "^2.5.0",
    "gatsby-source-contentful": "^3.0.6",
    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-transformer-sharp": "^2.5.21",
    "i18next": "^19.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.13.1",
    "react-i18next": "^11.7.3",
    "react-particles-js": "^3.4.1",
    "react-semantic-toasts": "^0.6.5",
    "react-syntax-highlighter": "^15.4.3",
    "react-typist": "^2.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "styled-components": "^5.2.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/react-typist": "^2.0.1",
    "@types/styled-components": "^5.1.5",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "gatsby-plugin-scss-typescript": "^4.0.13",
    "http-proxy-middleware": "^1.0.6",
    "netlify-lambda": "^2.0.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
